<component name="ProjectRunConfigurationManager">
  <configuration default="false" factoryName="Application" name="Run Application"
    type="Application">
    <envs>
      <env name="spring.profiles.active" value="local"/>
    </envs>
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="com.asr.experiment.query.dsl.example.*"/>
        <option name="ENABLED" value="true"/>
      </pattern>
    </extension>
    <method v="2">
      <option enabled="true" name="Make"/>
    </method>
    <module name="query.dsl.example.main"/>
    <option name="MAIN_CLASS_NAME" value="com.asr.experiment.query.dsl.example.Application"/>
  </configuration>
</component>